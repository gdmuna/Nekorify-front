    {
        "label": "性别",
        "fieldName": "sex",
        "required": true,
        "type": "radioGroup",
        "value": {
            "type": "string",
            "options": [
                {
                    "label": "男",
                    "value": "man"
                },
                {
                    "label": "女",
                    "value": "woman"
                }
            ]
        }
    },
