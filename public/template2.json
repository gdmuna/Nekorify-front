[
    {
        "label": "学号",
        "fieldName": "studentNumber",
        "required": true,
        "type": "input",
        "value": {
            "type": "number",
            "minLength": 11,
            "maxLength": 11
        },
        "style": {
            "inputType": "number"
        }
    },
    {
